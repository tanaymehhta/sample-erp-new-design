{
  "name": "polymer-trading-system",
  "homepage": "https://tanaymehhta.github.io/sample-erp-new-design",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "server": "tsx server/index.ts",
    "seed": "tsx prisma/seed.ts",
    "upload-deals": "tsx server/scripts/batchUploadDeals.ts",
    "test-sync": "tsx server/scripts/testSync.ts",
    "test-inventory": "tsx server/scripts/testInventory.ts",
    "backfill-inventory": "tsx server/scripts/backfillInventory.ts",
    "test-manual-selection": "tsx server/scripts/testManualSelection.ts",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@flydotio/litestream": "^1.0.1",
    "axios": "^1.4.0",
    "clsx": "^2.0.0",
    "date-fns": "^4.1.0",
    "framer-motion": "^10.16.4",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.4",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.14.2",
    "react-select": "^5.7.4",
    "tailwind-merge": "^1.14.0"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.7.10",
    "@prisma/client": "^5.1.1",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "express": "^4.18.2",
    "gh-pages": "^6.3.0",
    "googleapis": "^122.0.0",
    "postcss": "^8.4.27",
    "prisma": "^5.1.1",
    "tailwindcss": "^3.3.3",
    "tsx": "^3.12.7",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  },
  "dockerfile": {
    "litestream": true
  }
}
